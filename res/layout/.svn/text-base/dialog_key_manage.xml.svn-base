<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="800dp"
    android:layout_height="530dp"
    android:background="@drawable/dialog_bg" >
    <include 
        android:id="@+id/dialog_title"
        layout="@layout/dialog_title"
        />
    <LinearLayout 
        android:layout_below="@id/dialog_title"
        android:id="@+id/ll_key_manage_choose"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible"
        android:orientation="vertical"
        android:gravity="center_horizontal"
        >
        <TextView 
            android:text="当主密钥正在使用中一旦切换到\n下装密钥状态将清除所有已下装的\n主密钥是否继续？"
            android:textColor="#4dc6f6"
            android:textSize="28sp"
            android:layout_marginTop="50dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
        <LinearLayout 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="33dp"
        android:layout_marginBottom="48dp"
        android:layout_marginLeft="222dp"
        android:layout_marginRight="222dp"
        android:orientation="horizontal"
        >
        <ImageView 
            android:id="@+id/iv_dialog_key_cancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="55dp"
            android:src="@drawable/icon_quxiao"
            />
        <ImageView 
            android:id="@+id/iv_dialog_key_ok"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_ok"
            />
    </LinearLayout>
    </LinearLayout>
    
    <LinearLayout 
        android:layout_below="@id/dialog_title"
        android:id="@+id/ll_key_manage_success"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        android:orientation="vertical"
        android:gravity="center_horizontal"
        >
         <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/dialog_big"
            android:layout_marginTop="58dp"
            android:layout_marginBottom="35dp"
            />
         <TextView 
             android:text="主密钥导入成功"
             android:textSize="48sp"
             android:textColor="#4dc6f6"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_marginBottom="118dp"/>
        
      </LinearLayout>

</RelativeLayout>
