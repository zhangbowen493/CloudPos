package cn.wonders.pos_qdg.tool8583;
/**
 *  8583 每个域的报文编码方式
 */
public enum FieldCodeType {
	/**
	 * 十六进制小字节序
	 */
	H_LITTER_ENDIAN,
	/**
	 * 十六进制大字节序
	 */
	H_BIGGER_ENDIAN,
	/**
	 * bcd码 非压缩
	 */
	BCD_UNCOMPRESS,
	/**
	 * bcd码 压缩
	 */
	BCD_COMPRESS,
	/**
	 * bcd码 压缩 右补零
	 */
	BCD_COMPRESS_RIGHT,
	/**
	 * 八进制
	 */
	O,
	/**
	 * 二进制
	 */
	B,
	/**
	 * 十进制
	 */
	D, 
	/**
	 * ASCll码
	 */
	ASCll,
	/**
	 * 包含多个子域
	 */
	SUBFIELD
}
